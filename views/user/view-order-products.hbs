<section>
    <div class="container mt-5">
    <div class="row">
      {{#each products}}
        <div class="col-md-3 p-3">
            <div class="card" style="width: 15rem; height:24rem;">
  <img class="card-img-top" src="{{this.product.imageUrls.[0]}}" alt="Card image cap" style="height:12rem;">
                                    {{!-- <td><img style="width: 50px; height: 50px" src="{{this.product.thumbnailUrl}}" alt=""></td> --}}

  <div class="card-body">
    <h5 class="card-title">{{this.product.productName}}</h5>
    <p class="card-text">Rs:{{this.product.productPrice}}/-</p>
    <p class="card-text">Quantity:{{this.product.quantity}}/-</p>
    <p class="card-text">{{this.product.productDescription}}/-</p>

  </div>
</div>
        </div>
        {{/each}}

    </div>
    </div>
</section>